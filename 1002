import java.io.IOException;
 
import java.util.Scanner;


public class Main {

        public static void main(String[] args) {
        Scanner teclado = new Scanner(System.in);
        double A = teclado.nextDouble();
        double R = 3.14159;
        A = R*(A*A);
        
       System.out.printf("A=%.4f\n",(A));
        
    }
    
 
}
